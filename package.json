  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview --port 8080",
    "prepare": "husky && panda codegen",
    "start": "node index.js",
    "test": "node index.js",
    "lint": "eslint src",
    "lint:write": "eslint --debug src/ --fix",
